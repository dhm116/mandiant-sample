blue = #5B83AD
yellow = #FFFFE0

body
  padding 50px
  font 14px "Lucida Grande", Helvetica, Arial, sans-serif

  .template
    background linear-gradient(top, white, yellow)
    border-radius bottom 10px

  .helper
    border 1px solid blue

  .styled
    transform rotate(-30deg)
    margin-top 100px
    position absolute

  .panel-body
    padding 0px

  .pad-left
    padding-left 5px

  .commit h5
    margin 0px

  .authorship
    font-size: 12px
    color: #888

  .table tbody .highlight td
    background-color #E6F1F6
